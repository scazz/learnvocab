<div id="test-yourself">
	<div class="actions"> 
		{{touchable-icon classes="top-icon fa-eye" action="toggleQAVisibility"}}
	  {{touchable-icon classes="fa-thumbs-down bottom-icon" action="incorrect"}}
	</div>
	<div class="word-container">
	 <div class="word native">{{native}}</div>

		<div class="word translated" style="display: none">{{translated}}</div>
	</div>
	<div class="actions">
		{{touchable-icon classes="fa-refresh right-icon" action="toggleQAVisibilitySetting"}}
		{{touchable-icon classes="fa-thumbs-up bottom-icon right-icon" action="correct"}}
	</div>
</div> 
